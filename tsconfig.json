{
    "compilerOptions":{
        "outDir": "./public/js/app",
        "rootDir": "./src",
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false
    },
     "exclude": [
         "node_modules",
         "public",
         "servernode"
     ]
}